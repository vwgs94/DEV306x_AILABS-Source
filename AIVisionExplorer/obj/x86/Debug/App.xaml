<Application
    x:Class="AIVisionExplorer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AIVisionExplorer"
    xmlns:common="using:AIVisionExplorer.Common"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>
            <common:HasContextToVisibilityConverter x:Key="HasContextToVisibilityConverter"/>
            <common:HasListToVisibilityConverter x:Key="HasListToVisibilityConverter"/>
            <common:ListToLabelConverter x:Key="ListToLabelConverter"/>
            <common:DoubleToPercentageConverter x:Key="DoubleToPercentageConverter"/>
            <common:EmotionScoreLabelConverter x:Key="EmotionScoreLabelConverter"/>
            <common:HasValueToVisibilityConverter x:Key="HasValueToVisibilityConverter"/>            
        </ResourceDictionary>
    </Application.Resources>
    
</Application>

